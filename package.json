{
  "name": "5pro-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:all": "concurrently \"pnpm --filter @5pro/web dev\" \"pnpm --filter @5pro/api dev\"",
    "build": "next build",
    "build:all": "pnpm --filter @5pro/db build && pnpm --filter @5pro/ui build && pnpm --filter @5pro/api build && pnpm --filter @5pro/web build",
    "start": "next start",
    "test": "pnpm --filter @5pro/api test && pnpm --filter @5pro/web test",
    "test:e2e": "pnpm --filter @5pro/web test:e2e",
    "db:migrate": "pnpm --filter @5pro/db migrate",
    "db:seed": "pnpm --filter @5pro/db seed",
    "db:studio": "pnpm --filter @5pro/db studio",
    "setup": "pnpm install && pnpm db:migrate && pnpm db:seed",
    "lint": "pnpm --parallel --filter \"./apps/**\" --filter \"./packages/**\" lint"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@heroicons/react": "^2.2.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.4.1",
    "lucide-react": "^0.294.0",
    "next": "14.0.4",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.1.14",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "concurrently": "^8.2.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
